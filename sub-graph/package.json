{
  "name": "talent-layer",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "graphhelp": "graph help",
    "build": "graph build",
    "build:localhost": "graph build --network 0.0.0.0",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ trust-protocol",
    "create-local": "graph create --node http://0.0.0.0:8020/ trust-protocol",
    "remove-local": "graph remove --node http://0.0.0.0:8020/ trust-protocol",
    "deploy-local": "graph deploy --node http://0.0.0.0:8020/ --ipfs http://0.0.0.0:5001 trust-protocol --version-label 0.0.1",
    "start": "npm run codegen && npm run build:localhost && npm run remove-local && npm run create-local && npm run deploy-local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.25.1",
    "@graphprotocol/graph-ts": "^0.26.0"
  }
}
